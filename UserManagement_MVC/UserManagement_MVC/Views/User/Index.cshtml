@model IEnumerable<UserManagement_MVC.tbl_User>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Home", "Index")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Password)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmailID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tbl_Roles.RoleName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Password)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EmailID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbl_Roles.RoleName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PK_UserID }) |
            @Html.ActionLink("Details", "Details", new { id=item.PK_UserID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.PK_UserID })
        </td>
    </tr>
}

</table>
